{
  "name": "web-host",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "./web/host/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/web/host/browser",
        "index": "web/host/src/index.html",
        "baseHref": "/",
        "main": "./web/host/src/main.ts",
        "tsConfig": "web/host/tsconfig.app.json",
        "assets": ["web/host/src/favicon.ico", "web/host/src/assets"],
        "styles": ["web/host/src/styles.css"],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "./web/host/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "web/host/src/environments/environment.ts",
              "with": "web/host/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "./web/host/webpack.config.prod.js"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-ssr-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "browserTarget": "web-host:build:development",
        "serverTarget": "web-host:serve-server:development",
        "port": 4200,
        "browserTargetOptions": {
          "watch": true
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "browserTarget": "web-host:build:production",
          "serverTarget": "web-host:serve-server:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["./web/host/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "web-host:build",
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "web-host:build:development"
        },
        "production": {
          "buildTarget": "web-host:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "web/host/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "main": "web/host/server.ts",
        "outputPath": "dist/web/host/server",
        "outputFileName": "server.js",
        "tsConfig": "web/host/tsconfig.server.json",
        "compiler": "babel",
        "externalDependencies": "all",
        "outputHashing": "none",
        "isolatedConfig": true,
        "webpackConfig": "web/host/webpack.server.config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "web/host/src/environments/environment.ts",
              "with": "web/host/src/environments/environment.prod.ts"
            }
          ],
          "sourceMap": false
        }
      }
    },
    "serve-browser": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "web-host:build",
        "hmr": true,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "web-host:build:development"
        },
        "production": {
          "buildTarget": "web-host:build:production",
          "hmr": false
        }
      }
    },
    "serve-server": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "web-host:server:development",
        "buildTargetOptions": {
          "watch": true
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "web-host:server:production"
        }
      }
    }
  },
  "tags": []
}
